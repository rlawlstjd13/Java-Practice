public class AutoInc {
	public static void main(String[] args) {
		int a = 10;
		
		System.out.println(++a); // ++앞에붙이면 바로 나오고
		
		System.out.println(a++); // 뒤에 붙이면 한개더 만들어야함.
		System.out.println(a);
		
		//System.out.println(a);
		
		
		//복합 대입 연산자 += -= ...
